# 数据库配置 - 使用你的 MySQL 容器
DB_HOST=localhost:3306
DB_USERNAME=root
DB_PASSWORD=xxxx
DB_NAME=uniKeep

# Prisma 数据库连接字符串
DATABASE_URL="mysql://root:xxxx@localhost:3306/uniKeep"

# Redis 配置 - 使用你的 Redis 容器
REDIS_HOST=localhost:6379
