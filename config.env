# JWT 配置
JWT_SECRET=uniKeepSecret

# 环境配置
NODE_ENV=development

# Apollo 配置（已禁用）
APOLLO_ENABLED=false

# 微信小程序配置
WECHAT_MINI_APP_ID=wxabcd1234567890ef
WECHAT_MINI_APP_SECRET=你的AppSecret密钥32位字符串